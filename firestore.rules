rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Pricing Catalog - read/write for all (no auth yet)
    match /pricingCatalog/{id} {
      allow read, write: if true;
    }
    
    // Quotes collection - read/write for all (no auth yet)
    match /quotes/{quoteId} {
      allow read, write: if true;
    }
  }
}
